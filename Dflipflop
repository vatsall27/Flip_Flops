def AND(a,b,c):   
    return a&b&c
    
def OR(a,b): 
    return a|b

def NOT (x):
    return int(not x)
 
def NOR(a,b):  
    ans = OR(a,b)
    return NOT(ans)


 
D = int(input("Enter value of D = "))
R = NOT(D)
Qp = int(input("Enter value of Previous Output = "))



a1 = AND(R,1,1)
a2 = AND(D,1,1)


a3 = NOR(a2,Qp)
Qn = NOR(a1,a3)


print("value of Next Output is = ",Qn)
